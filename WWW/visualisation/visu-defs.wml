<define-tag visualisation-table whitespace=delete>
<preserve name>
<preserve description>
<preserve screenshot>
<set-var %attributes>

<TR>
  <TD bgcolor=#eeeeee width="30%" valign=top>
  <get-var name>
  </TD>

  <TD bgcolor=#eeeeee width="60%" valign=top>
  <get-var description>
  </TD>

  <TD bgcolor=#eeeeee width="10%" valign=top>
  <get-var screenshot>
  </TD>
  
</TR>

</define-tag>
